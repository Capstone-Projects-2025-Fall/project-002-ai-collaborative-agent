{
  "name": "ai-collab-agent",
  "displayName": "AI Collab Agent",
  "description": "Minimal starter: opens a webview to kick off team setup and task allocation.",
  "publisher": "your-publisher-id",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "aiCollab.openPanel",
        "title": "AI Collab Agent: Open"
      },
      {
        "command": "aiCollab.login",
        "title": "AI Collab Agent: Login"
      },
      {
        "command": "aiCollab.logout",
        "title": "AI Collab Agent: Logout"
      },
      {
        "command": "aiCollab.getSupabaseUser",
        "title": "AI Collab Agent: Get Supabase User"
      },
      {
        "command": "aiCollab.debugHello",
        "title": "AI Collab Agent: Hello (debug)"
      },
      {
        "command": "aiCollab.testEnv",
        "title": "AI Collab Agent: Test .env Variables"
      }
    ],
    "configuration": {
      "title": "AI Collab Agent",
      "properties": {
        "aiCollab.apiBaseUrl": {
          "type": "string",
          "default": "http://localhost:4000",
          "description": "Base URL of your Agent API (future use)."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -w -p ./",
    "package": "vsce package"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "dotenv": "^17.2.3"
  },
  "devDependencies": {
    "@types/node": "^20.11.20",
    "@types/vscode": "^1.80.0",
    "esbuild": "^0.23.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.8",
    "dotenv": "^16.4.5",
    "nanoid": "^4.0.2"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
